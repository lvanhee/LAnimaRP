<configuration>

	<display_parameters>
		<fullscreen value="false"></fullscreen>
		<display_area x="000" y="000" width = "1920" height = "1080"></display_area>
	</display_parameters>
	
    <displayed_animations>		     
		<enum_animation default = "normal">
    		<text_source>
				<file_location><hard_path path="https://docs.google.com/document/d/1qGPoIXvh5hGGxzwmAM1Z_QzCJ5slIMoHK1DL4V9x4IU/edit?usp=sharing"></hard_path>
				<refresh_period refresh_period="5000"></refresh_period>
				</file_location>
			</text_source>
			
			<displayed_animations name = "normal">
			    <image>
					<file_location><hard_path path="resources/images/hive/planning_avec_icones.png"></hard_path></file_location>
					<stretching><mode value = "stretch"></mode>	</stretching>
					<display_area x="0" y="0" width = "1920" height = "1080"></display_area>
				</image>
				<text_prompt unfolding_mode = "already_printed" color = "20,20,20">
    				<text_source>
						<file_location><hard_path path="https://docs.google.com/document/d/1ad-9ITRxdkJG8lix_vYAm1b2ll33W-OiCo9feXlsLGY/edit"></hard_path></file_location>
						<refresh_period refresh_period="5000"></refresh_period>
					</text_source>
					<display_area x="150" y="200" width = "840" height = "780"></display_area>
				</text_prompt>
				
				<text_prompt unfolding_mode = "already_printed" color = "20,20,20">
