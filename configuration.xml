						<refresh_period refresh_period="5000"></refresh_period>
					</text_source>
					<display_area x="920" y="200" width = "840" height = "780"></display_area>
				</text_prompt>
				
				<boolean_animation_displayer>
				<text_source info="input for boolean value" default="false" name = "print_red_med_bool">
					<file_location><hard_path path="https://docs.google.com/document/d/1MWNl6jKOhsod0zK7BOZmljSor_yVV7lKMh-UvkVi2u8/edit?usp=sharing"></hard_path>
					<refresh_period refresh_period="5000"></refresh_period>
					</file_location>
				</text_source>
				<displayed_animations>
					<image>
						<file_location><hard_path path="resources/images/hive/gas_mask_red.png"></hard_path></file_location>
						<stretching><mode value = "stretch"></mode>	</stretching>
						<display_area x="1605" y="200" width = "160" height = "160"></display_area>
					</image>
				</displayed_animations>
			</boolean_animation_displayer>
			
				<boolean_animation_displayer>
				<text_source info="input for boolean value" default="false" name = "print_red_med_bool">
					<file_location><hard_path path="https://docs.google.com/document/d/1BChqMpri9sxNXOViQ5-f3PlQtYPerH5Laqwl3rsDhqQ/edit"></hard_path>
					<refresh_period refresh_period="5000"></refresh_period>
					</file_location>
				</text_source>
				<displayed_animations>
					<image>
						<file_location><hard_path path="resources/images/hive/sport_red.png"></hard_path></file_location>
						<stretching><mode value = "stretch"></mode>	</stretching>
						<display_area x="1605" y="380" width = "160" height = "160"></display_area>
					</image>
				</displayed_animations>
			</boolean_animation_displayer>
		
			<boolean_animation_displayer>
				<text_source info="input for boolean value" default="false" name = "print_red_med_bool">
					<file_location><hard_path path="https://docs.google.com/document/d/1nQVWHSD3wzor3SoUT3dFoJgW5VpGrTj3jMNd7_eJA4k/edit"></hard_path>
					<refresh_period refresh_period="5000"></refresh_period>
					</file_location>
				</text_source>
				<displayed_animations>
					<image>
						<file_location><hard_path path="resources/images/hive/med_red.png"></hard_path></file_location>
						<stretching><mode value = "stretch"></mode>	</stretching>
						<display_area x="1606" y="563" width = "161" height = "160"></display_area>
					</image>
				</displayed_animations>
			</boolean_animation_displayer>
			</displayed_animations>
						
			<displayed_animations name = "alert">
				<sound sound_mode="repeat">
					<file_location><hard_path path="resources/sound/alerte.wav"></hard_path></file_location>
				</sound>
				<image>
					<file_location><hard_path path="resources/images/gifs/radioactive_alert.gif"></hard_path></file_location>
					<stretching><mode value = "stretch"></mode>	</stretching>
					<display_area x="0" y="0" width = "1920" height = "1080"></display_area>
				</image>
			</displayed_animations>
			
			<displayed_animations name = "hack">
				<xml_textsource_animation>
					<file_location><hard_path path="resources/configuration_file_examples/technical_demos/no_action_hack.xml"></hard_path></file_location>
				</xml_textsource_animation>
			</displayed_animations>
		</enum_animation>	
     </displayed_animations> 
</configuration>